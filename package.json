{
  "name": "@daostack/client",
  "version": "0.0.4",
  "description": "",
  "keywords": [],
  "main": "dist/lib/index.js",
  "typings": "dist/types/index.d.ts",
  "files": [
    "dist",
    "src"
  ],
  "author": "Matan Tsuberi <tsuberim@gmail.com>",
  "repository": {
    "type": "git",
    "url": ""
  },
  "license": "MIT",
  "engines": {
    "node": ">=6.0.0"
  },
  "scripts": {
    "build": "tsc && typedoc --out docs --target commonjs --theme minimal --mode file src",
    "build:watch": "tsc --watch",
    "bundle": "rollup -c rollup.config.ts -w",
    "commit": "git-cz",
    "deploy-docs": "ts-node tools/gh-pages-publish",
    "lint": "npm run tslint",
    "prebuild": "rimraf dist",
    "quality": "npm run test:prod",
    "report-coverage": "cat ./coverage/lcov.info | coveralls",
    "semantic-release": "semantic-release",
    "semantic-release-prepare": "ts-node tools/semantic-release-prepare",
    "setup-env": "npm run subgraph:deploy",
    "subgraph:deploy": "npm explore @daostack/subgraph -- npm i && npm explore @daostack/subgraph -- npm run deploy",
    "subgraph:test": "npm explore @daostack/subgraph -- npm run migrate:development && npm explore @daostack/subgraph -- npm run deploy && npm run test Subscription",
    "test": "jest --forceExit",
    "test:watch": "jest --watch",
    "tslint": "tslint  --project tsconfig.json -t codeFrame 'src/**/*.ts' 'test/**/*.ts'"
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-conventional-changelog"
    }
  },
  "jest": {
    "transform": {
      ".(ts|tsx)": "ts-jest"
    },
    "testEnvironment": "node",
    "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(ts|tsx|js)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ],
    "coveragePathIgnorePatterns": [
      "/node_modules/",
      "/test/"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 10,
        "functions": 10,
        "lines": 10,
        "statements": 10
      }
    },
    "collectCoverage": false,
    "collectCoverageFrom": [
      "src/*.{js,ts}"
    ]
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "devDependencies": {
    "@commitlint/cli": "^7.1.2",
    "@commitlint/config-conventional": "^7.1.2",
    "@daostack/migration": "0.0.0-alpha.58-v5",
    "@daostack/subgraph": "git+https://github.com/daostack/subgraph.git",
    "@types/jest": "^23.3.2",
    "@types/node": "^10.11.0",
    "babel-eslint": "^10.0.1",
    "colors": "^1.3.2",
    "commitizen": "^3.0.0",
    "coveralls": "^3.0.2",
    "cross-env": "^5.2.0",
    "cz-conventional-changelog": "^2.1.0",
    "eslint": "^5.10.0",
    "husky": "^1.0.1",
    "jest": "^23.6.0",
    "jest-config": "^23.6.0",
    "lint-staged": "^7.3.0",
    "lodash.camelcase": "^4.3.0",
    "prompt": "^1.0.0",
    "replace-in-file": "^3.4.2",
    "rimraf": "^2.6.2",
    "rollup": "^0.66.2",
    "rollup-plugin-commonjs": "^9.1.8",
    "rollup-plugin-json": "^3.1.0",
    "rollup-plugin-node-resolve": "^3.4.0",
    "rollup-plugin-sourcemaps": "^0.4.2",
    "rollup-plugin-typescript2": "^0.17.0",
    "semantic-release": "^15.9.16",
    "spawn-command": "0.0.2-1",
    "ts-jest": "^23.10.2",
    "ts-node": "^7.0.1",
    "tslint": "^5.11.0",
    "tslint-config-standard": "^8.0.1",
    "typedoc": "^0.12.0",
    "typescript": "^3.0.3"
  },
  "dependencies": {
    "@types/graphql": "^14.0.3",
    "@types/isomorphic-fetch": "0.0.34",
    "@types/web3": "^1.0.12",
    "@types/ws": "^6.0.1",
    "apollo-cache-inmemory": "^1.3.11",
    "apollo-client": "^2.4.7",
    "apollo-client-ws": "^2.3.0",
    "apollo-link-http": "^1.5.7",
    "apollo-link-ws": "^1.0.10",
    "axios": "^0.18.0",
    "graphql": "^14.0.2",
    "graphql-tag": "^2.10.0",
    "hdwallet-accounts": "0.0.1",
    "isomorphic-fetch": "^2.2.1",
    "isomorphic-ws": "^4.0.1",
    "moment": "^2.22.2",
    "node-gyp": "^3.8.0",
    "rxjs": "^6.3.3",
    "subscriptions-transport-ws": "^0.9.15",
    "web3": "^1.0.0-beta.36",
    "ws": "^6.1.2"
  }
}
